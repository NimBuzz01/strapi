{
  "kind": "collectionType",
  "collectionName": "experience_bundles",
  "info": {
    "singularName": "experience-bundle",
    "pluralName": "experience-bundles",
    "displayName": "ExperienceBundle",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bundleId": {
      "type": "uid"
    },
    "name": {
      "type": "string"
    },
    "experiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience.experience"
    },
    "cost": {
      "type": "component",
      "repeatable": false,
      "component": "experience.rates"
    },
    "duration": {
      "type": "component",
      "repeatable": false,
      "component": "experience.duration"
    },
    "childCostReduction": {
      "type": "component",
      "repeatable": false,
      "component": "experience.rates"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
